[
    {
        "name": "QQPush",
        "description": "QQ手表版推送消息, 唤醒标准版QQ",
        "priority": 1,
        "condition": "notificationAdded == true && pkgName == \"com.tencent.qqlite\"",
        "actions": [
            "activity.launchProcessForPackage(\"com.tencent.mobileqq\")",
            "su.exe(\"am start-foreground-service com.tencent.mobileqq/com.tencent.mobileqq.msf.service.MsfService\")",
            "killer.killPackage(\"com.tencent.qqlite\")"
        ]
    }
]